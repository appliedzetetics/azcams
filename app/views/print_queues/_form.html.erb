<%= simple_form_for(@print_queue) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :entity %>
    <%= f.input :entity_id %>
    <%= f.association :printtemplate %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
