<%= simple_form_for(@temporary_import) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :file_no %>
    <%= f.input :forename %>
    <%= f.input :surname %>
    <%= f.input :dob %>
    <%= f.input :phone %>
    <%= f.input :address1 %>
    <%= f.input :address2 %>
    <%= f.input :town %>
    <%= f.input :postcode %>
    <%= f.input :referred_by %>
    <%= f.input :dateref_text %>
    <%= f.input :referred_date %>
    <%= f.input :ia_counsellor %>
    <%= f.input :ia_location %>
    <%= f.input :ia_date %>
    <%= f.input :counsellor %>
    <%= f.input :location %>
    <%= f.input :counselling_start_date %>
    <%= f.input :status %>
    <%= f.input :rogue_address %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
